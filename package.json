{
  "name": "svgpack",
  "version": "2.1.0",
  "homepage": "https://github.com/blivesta/svgpack",
  "description": "SVG sprites and CSS and generate the HTML page.",
  "main": "index.js",
  "bin": {
    "svgpack": "bin/cli.js"
  },
  "scripts": {
    "cli": "svgpack test/fixtures/*.svg -d test/build -n cli -p sp -m test/templates/html/fixture.html -c test/templates/css/fixture.css -s test/templates/svg/fixture.svg",
    "dev": "npm run lint && mocha -w -R spec",
    "lint": "standard",
    "start": "npm run lint && npm run cli && npm run dev",
    "test": "npm run lint && mocha -R spec"
  },
  "dependencies": {
    "cheerio": "^0.20.0",
    "datauri": "^1.0.4",
    "glob": "^7.0.3",
    "html-to-json": "^0.6.0",
    "lodash": "^4.9.0",
    "minimist": "^1.1.2",
    "mkdirp": "^0.5.1",
    "object-assign": "^4.0.1",
    "svgo": "^0.6.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "postcss": "^5.0.19",
    "rimraf": "^2.5.2",
    "sircus": "^2.0.1",
    "standard": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:blivesta/svgpack.git"
  },
  "author": {
    "name": "blivesta",
    "url": "http://www.blivesta.com/"
  },
  "license": "MIT"
}
