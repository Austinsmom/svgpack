{
  "name": "svgpack",
  "version": "1.0.1",
  "homepage": "https://github.com/blivesta/svgpack",
  "description": "Generator for SVG sprite and icons preview.",
  "main": "index.js",
  "bin": {
    "svgpack": "bin/cli.js"
  },
  "scripts": {
    "cli": "svgpack test/fixtures/*.svg --dest ~build --name cli --prefix c --templateHtml test/templates/html/fixture.html --templateCss test/templates/css/fixture.css --templateSvgSprite test/templates/svg/fixture.svg",
    "dev": "mocha -w -R spec",
    "lint": "eslint lib/svgpack.js",
    "start": "npm run lint && npm run cli && npm run dev",
    "test": "mocha -R spec"
  },
  "dependencies": {
    "cheerio": "^0.20.0",
    "datauri": "^1.0.4",
    "glob": "^7.0.3",
    "html-to-json": "^0.6.0",
    "lodash": "^4.9.0",
    "minimist": "^1.1.2",
    "mkdirp": "^0.5.1",
    "object-assign": "^4.0.1",
    "svgo": "^0.6.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^2.5.3",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "sircus": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:blivesta/svgpack.git"
  },
  "author": {
    "name": "blivesta",
    "url": "http://www.blivesta.com/"
  },
  "license": "MIT"
}
